{
  "messages": {
    "error": {
      "title": "Error",
      "message": "Gulp reported error: <%= error.message %>!"
    },
    "success": {
      "title": "Success",
      "message": "Gulp tasks completed successfully!"
    }
  },
  "sass": {
    "src": "../sass-bootstrap/**/*.scss",
    "destination": "../css",
    "sourcemaps": "../css",
    "config": {
      "style": "compressed",
      "errLogToConsole": true,
      "includePaths": []
    },
    "autoprefixer": [
      "last 2 versions",
      "> 1%",
      "ie 9",
      "ie 10"
    ]
  },
  "css": {
    "src": ["../css/bootstrapstyle.css"],
    "thirdpartysrc": ["../css/thirdparty/aos.css" , "../css/thirdparty/test.css" ],
    "dest": "../css/dist"
  },

  "resources": {
    "css": {
      "src:": "../css/dist/**/*.css",
      "dest:": "../../../public/resources/themes/suilven_default/css/"
    }
  }
}
